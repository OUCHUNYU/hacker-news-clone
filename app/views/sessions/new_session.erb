<h3>Create a new user</h3>
<form action="/users" method="POST" id="sign-up">
  username:<br>
  <input type="text" name="users[username]" id="signup-username">
  <br>
  email:<br>
  <input type="text" name="users[email]" id="signup-email">
  <br>
  password:<br>
  <input type="password" name="users[password]" id="signup-password">
  <br>
  <br>
  <input type="submit" value="Create">
</form>
