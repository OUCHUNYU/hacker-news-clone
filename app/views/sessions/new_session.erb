<h3>Login</h3>
<form action="/sessions" method="POST" id="sign-in">
  email:<br>
  <input type="text" name="sessions[email]" id="signin-email">
  <br>
  password:<br>
  <input type="password" name="sessions[password]" id="signin-password">
  <br>
  <br>
  <input type="submit" value="Login">
</form>


